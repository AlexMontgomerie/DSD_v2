// Test bench for fp multiplier

`timescale 1ns / 1ns
module test_ahfp_mult;

reg clk, reset;
wire [31:0] dataa,datab,result;

ahfp_mult dut (	.dataa(dataa),
		.datab(datab),
		.result(result));

// clock generator
initial
	begin
		clk = 0;
		forever #10 clk = !clk;
	end

// test cases
initial
	begin
		//test 1
		//test 2
		//test 3
	end

initial
	$monitor($stime,,reset,,clk,,dataa,,datab,,result);

endmodule
